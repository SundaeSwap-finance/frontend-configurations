  "license": "MIT",
  "files": [
    "dist",
    "package.json"
  ],
  "keywords": [
    "webpack"
  ],
  "private": true,
  "scripts": {
    "build": "yarn run clean && yarn run compile",
    "compile": "tsc -p ./tsconfig.json",
