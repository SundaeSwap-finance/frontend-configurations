    "mini-css-extract-plugin": "^2.6.0",
    "react-dev-utils": "^12.0.1",
    "string-replace-loader": "^3.1.0",
    "ts-loader": "^9.3.0"
  },
  "peerDependencies": {
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  },
  "devDependencies": {
    "@types/react-dev-utils": "^9.0.10"
  }
}
