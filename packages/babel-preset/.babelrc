{
  "plugins": ["@babel/plugin-proposal-class-properties"],
  "env": {
    "esmUnbundled": {
      "presets": ["@babel/preset-typescript"]
    },
    "esmBundled": {
      "presets": [
        [
          "@babel/preset-env",
          { "targets": "> 0.25%, not dead", "modules": false }
        ],
        "@babel/preset-typescript"
      ]
    },
    "cjs": {
      "presets": [
        ["@babel/preset-env", { "modules": "commonjs" }],
        "@babel/preset-typescript"
      ]
    }
  }
}
